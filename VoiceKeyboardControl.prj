    dout.write(v.length + &quot;\n&quot;);
    for (int j = 0; j &lt;  v.length; j++){
      dout.write(v[j] + &quot;\n&quot;);
    }
  }
  dout.write(&quot;END\n&quot;);
  dout.close();
} catch( Exception e ){dprint(e+&quot;\n&quot;);}
